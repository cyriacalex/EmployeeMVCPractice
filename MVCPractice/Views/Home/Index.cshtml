@model IEnumerable<MVCPractice.Models.Employee>
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h3>Employee Info</h3>
</div>
<div class="container">
    <div class="row">
        <div class="col-sm-8">
            <table class="table table-striped" border="1">
                <tr>
                    <th>Employee ID</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Email</th>
                    <th>Date Joined</th>
                    <th>Salary</th>
                </tr>
                @foreach (var item in Model)
                {
                    <tr>
                        <td> @Html.DisplayFor(modelItem2547 => item.eID)</td>                        
                        <td> @Html.DisplayFor(asdf => item.firstName)</td>
                        <td> @Html.DisplayFor(hjkl => item.lastName)</td>
                        <td> @Html.DisplayFor(qwer => item.email) </td>
                        <td> @Html.DisplayFor(dfgh => item.dateJoined)</td>
                        <td> @Html.DisplayFor(wert => item.salary)</td>
                    </tr>
                }
            </table>
        </div>
    </div>
</div>
